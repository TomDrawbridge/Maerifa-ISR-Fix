{
  "name": "tmp-cap",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "update-plasmic": "npm update @plasmicapp/cli @plasmicapp/react-web",
    "plasmic-sync": "plasmic sync --yes --force",
